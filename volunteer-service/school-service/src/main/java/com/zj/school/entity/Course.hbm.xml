<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zj.school.entity.Course" table="Course">
        <id name="id" column="CId" type="java.lang.String"></id>
        <property name="name" type="java.lang.String" column="Cname"></property>
        <many-to-one name="teacher" column="TId" class="com.zj.school.entity.Teacher"></many-to-one>
        <set name="sc" table="SC" lazy="extra">
            <key column="CId"></key>
            <one-to-many class="com.zj.school.entity.Sc"></one-to-many>
        </set>
     </class>
</hibernate-mapping>