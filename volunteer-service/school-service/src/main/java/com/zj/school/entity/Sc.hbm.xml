<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zj.school.entity.Sc" table="SC">
        <composite-id>
            <key-many-to-one name="course" class="com.zj.school.entity.Course" column="CId"></key-many-to-one>
            <key-many-to-one name="student" class="com.zj.school.entity.Student" column="SId"></key-many-to-one>
        </composite-id>
        <property name="score" type="java.lang.Integer" column="score"></property>
    </class>
</hibernate-mapping>