<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zj.school.entity.Teacher" table="Teacher" dynamic-update="true" dynamic-insert="true">
        <id name="id">
            <column name="TId"></column>
        </id>
        <property name="name">
            <column name="Tname"></column>
        </property>
        <set name="course" lazy="extra">
            <key column="CId"></key>
            <one-to-many class="com.zj.school.entity.Course"></one-to-many>
        </set>
    </class>
</hibernate-mapping>